Console.Clear();
        Console.WriteLine("Velkommen til mit spil. Håber du har styr på S.H.I.E.L.D. ellers bliver det måske ikke så sjovt");

        do
        {
            
            Console.WriteLine(CurrentRoom[currentRoomNumber].RoomDescription);
            roomSwitch = Console.ReadLine();

            Console.Clear();

            switch (roomSwitch)
            {

                case "1":
                    {
                        QuizRoom.MiniQuiz(currentMiniQuiz[Kjole.UsesLeft], Hints.UsesLeft);
                        Console.ReadKey();
                        break;
                    }

                case "2":
                    {
                        Console.WriteLine(SkyeChip.ItemDescription);
                        Hints.FindHints(SkyeChip.UsesLeft);
                        Console.ReadKey();
                        break;
                    }
                //Lola
                case "3":
                    {

                        Console.ReadKey();
                        break;
                    }
                //Dør
                case "4":
                    {
                        QuizRoom.Quiz(CurrentQuiz, ref currentRoomNumber, Hints.UsesLeft);
                        Console.ReadKey();
                        break;
                    }
                default:
                    {
                        Console.WriteLine("Der er sket en fejl. Tryk enter og prøv venligst igen");
                        Console.ReadKey();
                        Console.Clear();
                        break;
                    }
            }

        

        
        
}
        while (roomLoop);




        Console.ReadKey();
